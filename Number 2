#include <iostream>

using namespace std;

float pow(float a, int b)
{
  if(a==0)return 0;
  if(b==0)return 1;
  if(b<0) return pow(a, b+1)/a;
  if(b>0) return pow(a,b-1)*a;
}

int main()
{
  float pow(float a, int b);
  int b;
  float a,c;
  cin >> a >> b;
  c = pow(a, b);
  cout << a << "^" << b << "=" << c;
}
